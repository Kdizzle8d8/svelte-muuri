{
  "bracketSameLine": true,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "svelteBracketNewLine": false,
        "svelteIndentScriptAndStyle": true,
        "svelteSortOrder": "scripts-markup-styles-options",
        "svelteStrictMode": false
      }
    },
    {
      "files": "*.md",
      "options": {
        "printWidth": 79,
        "tabWidth": 2,
        "useTabs": false
      }
    },
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    }
  ],
  "pluginSearchDirs": false,
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
  "printWidth": 100,
  "singleQuote": false,
  "trailingComma": "none",
  "useTabs": false
}
